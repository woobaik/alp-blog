<!DOCTYPE html>
<html>
  <head>
    <title>AlphaBlog</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <% flash.each do |err,msg| %>
    <ul>
      <li><%= msg %></li>
    </ul>
    <% end %>
    <%= yield %>
  </body>
</html>
